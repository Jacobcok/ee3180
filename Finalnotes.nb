       RowBox[{"I", "*", "2", "*", "Pi", "*", "f", "*", "tau"}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"tau", ",", 
       RowBox[{"-", "Infinity"}], ",", "Infinity"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"h", "=", 
   RowBox[{"B", "*", 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", "t"}], "/", "T"}], "]"}], "*", 
    RowBox[{"UnitStep", "[", "t", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"H", "=", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{"h", "*", 
      RowBox[{"Exp", "[", 
       RowBox[{
        RowBox[{"-", "I"}], "*", "2", "*", "Pi", "*", "f", "*", "t"}], 
       "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", 
       RowBox[{"-", "Infinity"}], ",", "Infinity"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SYY", "=", 
   RowBox[{"SXX", "*", 
    RowBox[{
     RowBox[{"Abs", "[", "H", "]"}], "^", "2"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Px", "=", 
   RowBox[{"Integrate", "[", 
    RowBox[{"SXX", ",", 
     RowBox[{"{", 
      RowBox[{"f", ",", 
       RowBox[{"-", "Infinity"}], ",", "Infinity"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Py", "=", 
  RowBox[{"Integrate", "[", 
   RowBox[{"SYY", ",", 
    RowBox[{"{", 
     RowBox[{"f", ",", 
      RowBox[{"-", "Infinity"}], ",", "Infinity"}], "}"}]}], 
   "]"}]}]}], "Input",
