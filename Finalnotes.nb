      RowBox[{"(", 
       RowBox[{"a", "^", "k"}], ")"}]}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"k", ",", " ", "k1", ",", " ", "k2"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"c", " ", "=", " ", 
   RowBox[{"1", "/", "S"}]}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"test", " ", "if", " ", "sum"}], "=", "1"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"pX", "=", 
   RowBox[{"c", "/", 
    RowBox[{"a", "^", "k"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"pXsum", " ", "=", " ", 
   RowBox[{"Sum", "[", 
    RowBox[{"pX", ",", " ", 
     RowBox[{"{", 
      RowBox[{"k", ",", " ", "k1", ",", " ", "k2"}], "}"}]}], "]"}]}], 
  ";"}]}], "Input",
