    RowBox[{"test", " ", "if", " ", "sum"}], "=", "1"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"pX", "=", 
   RowBox[{"c", "/", 
    RowBox[{"a", "^", "k"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"pXsum", " ", "=", " ", 
