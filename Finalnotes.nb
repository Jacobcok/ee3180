 RowBox[{
  RowBox[{"a", "=", 
   RowBox[{"-", "3.6"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"fX", "[", "x_", "]"}], "=", 
