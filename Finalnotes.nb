         RowBox[{"(", 
          RowBox[{"s", "-", "1"}], ")"}], "/", "sigma"}], ")"}], "^", "2"}]}],
      "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"fSg2", "=", 
   RowBox[{
    RowBox[{"1", "/", 
     RowBox[{"Sqrt", "[", 
      RowBox[{"2", "*", "Pi", "*", 
       RowBox[{"sigma", "^", "2"}]}], "]"}]}], "*", 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"-", "1"}], "/", "2"}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"s", "-", "2"}], ")"}], "/", "sigma"}], ")"}], "^", "2"}]}],
      "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"fSg3", "=", 
   RowBox[{
    RowBox[{"1", "/", 
     RowBox[{"Sqrt", "[", 
      RowBox[{"2", "*", "Pi", "*", 
       RowBox[{"sigma", "^", "2"}]}], "]"}]}], "*", 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"-", "1"}], "/", "2"}], 
